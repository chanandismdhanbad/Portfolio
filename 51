string_1 = input('enter first string:\t')
new=''
for i in string_1:
	if i==' ':
		pass
	else:
		new = new+i
print (new)

string_2 = input('enter second string:\t')
string_3 = ''
for i in new:
	if i not in string_2:
		break
	else:
		string_3 = string_3+i

if new==string_3:
	print (new)
else:
	print ('characters are missing')
