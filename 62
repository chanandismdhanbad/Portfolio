string_1 = list(input('enter first string:\t'))
string_2 = list(input('enter second string:\t'))

for i in range(len(string_1)):
	for j in range(len(string_2)):
		if string_2[j]==string_1[i]:
			string_2[j]=''
		else:
			pass

new = ''
for i in string_2:
	if i=='':
		pass
	else:
		new = new+i
print (new)
