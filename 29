string_1 = input('Enter any sentence:\t')
string_2 = input('Enter the word/character to insert:\t')
indx = int(input('Enter the location:\t'))

list_1=[]
for words in string_1.split():
	list_1.append(words)
list_1.insert(indx-1,string_2)

string_1=''
for i in list_1:
	string_1= string_1+' '+i
print (string_1.strip())
