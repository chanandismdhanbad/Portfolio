string_1 = input('Enter any sentence:-')

list_1=[]
for words in string_1.split():
	if string_1.count(words)>1:
		list_1.append(words)
if (list_1==[]):
	print ('No repeating sequences')
else:
	list_2=[]
	for i in list_1:
		list_2.append(len(i))

	indx= list_2.index(max(list_2))
	print ('longest repeating sequence in above string is \''+list_1[indx]+'\'')
